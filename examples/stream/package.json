    "url": "git+https://github.com/WindRunnerMax/BlockKit.git"
  },
  "homepage": "https://github.com/WindRunnerMax/BlockKit",
  "bugs": {
    "url": "https://github.com/WindRunnerMax/BlockKit/issues"
  },
  "dependencies": {
    "marked": "15.0.8",
    "@block-kit/delta": "workspace: *",
    "@block-kit/utils": "workspace: *"
  },
  "devDependencies": {
    "concurrently": "8.2.2"
  }
